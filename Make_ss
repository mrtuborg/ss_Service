cp  funcs/$1/$1_functions.cpp src/Functions/functions.cpp
cp  funcs/$1/$1_functions.h src/Functions/functions.h
cp  funcs/$1/$1_SpecFuncs.cpp src/Functions/SpecFuncs.cpp

./funcs/$1/prebuild funcs/$1

rm -f src/Makefile.am
cat  src/_Makefile.am funcs/$1/Makefile.add src/Makefile.add >> src/Makefile.am
cat  src/_global.cpp funcs/$1/global.cpp >> src/global.cpp               
cat  src/_global.h funcs/$1/global.h >> src/global.h 

#cp src/Makefile.am make.out

cp src/_auto_config.h src/auto_config.h  
REV="#define REVISION \""`svnversion`"\"" >> src/auto_config.h
echo $REV >> src/auto_config.h
make

mv  src/ss_Service s$1_Service
chmod +x s$1_Service

rm -f src/Functions/functions.cpp
rm -f src/Functions/functions.h
rm -f src/Functions/SpecFuncs.cpp
rm -f src/auto_config.h
./funcs/$1/clean